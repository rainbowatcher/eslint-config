{
    "compilerOptions": {
        "target": "ESNext",
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "baseUrl": ".",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
    ]
}
